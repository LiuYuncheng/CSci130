package main
import "fmt"

func Min (x []float64) float64{
min:=x[0]
for i:=0; i<len(x); i++{
if x[i]<min{
min=x[i]
}
}
return min
}

func Max (x []float64) float64{
max:=x[0]
for i:=0; i<len(x); i++{
if x[i]>max{
max=x[i]
}
}
return max
}

func main(){
s:=[]float64{3.0,2.0,5.0}
fmt.Println(Min(s))
fmt.Println(Max(s))
}
